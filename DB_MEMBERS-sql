
CREATE TABLE GENDER
(
  GENDER_  VARCHAR NOT NULL COMMENT '성별 코드',
  UNIQE_ID VARCHAR NOT NULL COMMENT '유니크 _ID',
  PRIMARY KEY (UNIQE_ID)
) COMMENT '성별 코드';

CREATE TABLE HOBBY_CODE
(
  UNIQE_ID              NOT NULL COMMENT '유니크_ ID',
  HOBBY_CODE_ID VARCHAR NOT NULL COMMENT '취미 명',
  PRIMARY KEY (UNIQE_ID)
) COMMENT '취미 코드';

CREATE TABLE MEMBERS
(
  NAME      VARCHAR     NOT NULL COMMENT '이름',
  AGE       INESTRING   NOT NULL COMMENT '나이',
  GENDER    VARCHAR     NOT NULL COMMENT '성별',
  TELEPHONE VARCHAR     NOT NULL COMMENT '전화번호',
  UNIQE_ID  VARCHAR(50) NOT NULL COMMENT '유니크_ID',
  ADDRESS   VARCHAR     NOT NULL COMMENT '주소',
  UNIQE_ID  VARCHAR     NOT NULL COMMENT '유니크 _ID',
  PRIMARY KEY (UNIQE_ID)
) COMMENT '회원정보';

CREATE TABLE MEMBERS_HOBBY
(
  HOBBY         VARCHAR(500) NOT NULL COMMENT '취미',
  MEMBERS_HOBBY VARCHAR      NOT NULL COMMENT '회원_취미ID',
  UNIQE_ID      VARCHAR      NOT NULL COMMENT '유니크ID',
  UNIQE_ID      VARCHAR(50)  NOT NULL COMMENT '유니크ID',
                             NOT NULL,
                             NOT NULL
) COMMENT '회원_ 취미';

CREATE TABLE MEMBERS_LOGIN
(
  EMAIL            VARCHAR     NOT NULL COMMENT '이메일',
  UNIQE_ID         VARCHAR(50) NOT NULL COMMENT '유니크_ID',
  MEMBERS_LOGIN_ID VARCHAR     NOT NULL COMMENT '회원 로그인_ID',
  UNIQE_ID         VARCHAR(50) NOT NULL COMMENT '유니크_ID'
) COMMENT '회원 로그인';

ALTER TABLE MEMBERS_HOBBY
  ADD CONSTRAINT FK_MEMBERS_TO_MEMBERS_HOBBY
    FOREIGN KEY (UNIQE_ID)
    REFERENCES MEMBERS (UNIQE_ID);

ALTER TABLE MEMBERS_HOBBY
  ADD CONSTRAINT FK_HOBBY_CODE_TO_MEMBERS_HOBBY
    FOREIGN KEY ()
    REFERENCES HOBBY_CODE (UNIQE_ID);

ALTER TABLE MEMBERS_LOGIN
  ADD CONSTRAINT FK_MEMBERS_TO_MEMBERS_LOGIN
    FOREIGN KEY (UNIQE_ID)
    REFERENCES MEMBERS (UNIQE_ID);

ALTER TABLE MEMBERS
  ADD CONSTRAINT FK_GENDER_TO_MEMBERS
    FOREIGN KEY (UNIQE_ID)
    REFERENCES GENDER (UNIQE_ID);
